		"cstddef": "cpp",
		"cstdint": "cpp",
		"cstdio": "cpp",
		"cstdlib": "cpp",
		"cstring": "cpp",
		"ctime": "cpp",
		"cwchar": "cpp",
		"deque": "cpp",
		"exception": "cpp",
		"fstream": "cpp",
		"initializer_list": "cpp",
		"iomanip": "cpp",
		"ios": "cpp",
		"iosfwd": "cpp",
		"iostream": "cpp",
		"istream": "cpp",
		"iterator": "cpp",
		"limits": "cpp",
		"memory": "cpp",
		"new": "cpp",
		"numeric": "cpp",
		"ostream": "cpp",
		"queue": "cpp",
		"ratio": "cpp",
		"set": "cpp",
		"sstream": "cpp",
		"stack": "cpp",
